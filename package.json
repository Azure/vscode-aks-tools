{
    "name": "vscode-aks-tools",
    "displayName": "Azure Kubernetes Service",
    "description": "Display Azure Kubernetes Services within VS Code",
    "version": "0.0.4",
    "preview": true,
    "aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
    "publisher": "ms-kubernetes-tools",
    "icon": "resources/aks-tools.png",
    "engines": {
        "vscode": "^1.33.0"
    },
    "license": "MIT",
    "categories": [
        "Azure"
    ],
    "keywords": [
        "kubernetes",
        "azure",
        "aks",
        "acs",
        "kubernetes-extension-cloud-provider"
    ],
    "activationEvents": [
        "onView:kubernetes.cloudExplorer",
        "onCommand:aks.detectorDiagnostics"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "aks.selectSubscriptions",
                "title": "Select subscriptions...",
                "icon": {
                    "dark": "resources/dark/filter.svg",
                    "light": "resources/light/filter.svg"
                }
            },
            {
                "command": "aks.detectorDiagnostics",
                "title": "Run AKS Diagnostics"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "aks.selectSubscriptions",
                    "when": "never"
                }
            ],
            "view/item/context": [
                {
                    "command": "aks.selectSubscriptions",
                    "when": "viewItem == aks.subscription",
                    "group": "inline"
                },
                {
                    "command": "aks.detectorDiagnostics",
                    "when": "view == kubernetes.cloudExplorer && viewItem =~ /aks\\.cluster/i"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "extensionDependencies": [
        "ms-vscode.azure-account",
        "ms-kubernetes-tools.vscode-kubernetes-tools"
    ],
    "devDependencies": {
        "@types/js-yaml": "^3.12.1",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.9",
        "tslint": "^5.20.1",
        "typescript": "^3.7.2",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@azure/arm-containerservice": "11.0.0",
        "azure-arm-resource": "^7.3.0",
        "handlebars": "^4.7.6",
        "js-yaml": "^3.13.1",
        "vscode-azureextensionui": "^0.27.1",
        "vscode-kubernetes-tools-api": "^1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Azure/vscode-aks-tools"
    }
}
