        "cross-fetch": "^3.1.5",
        "decompress": "^4.2.1",
        "download": "^8.0.0",
        "js-yaml": "^4.1.0",
        "move-file": "^3.0.0",
        "rxjs": "^7.8.0",
        "semver": "^7.5.2",
        "sinon": "^12.0.1",
        "tmp": "^0.2.1",
        "vscode-extension-telemetry": "^0.1.6",
        "vscode-kubernetes-tools-api": "^1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Azure/vscode-aks-tools"
    }
}
