                {
                    "command": "aks.aksBestPracticesDiagnostics",
                    "group": "navigation"
                },
                {
                    "command": "aks.aksIdentitySecurityDiagnostics",
                    "group": "navigation"
                },
                {
                    "command": "aks.aksNodeHealthDiagnostics",
